{
  "name": "n8n-nodes-sunx-perps",
  "version": "1.0.0",
  "description": "n8n node for SunX Perpetual Futures Trading",
  "keywords": [
    "n8n-community-node-package",
    "sunx",
    "perps",
    "futures",
    "trading",
    "crypto"
  ],
  "license": "MIT",
  "homepage": "https://github.com/deepakdhaka-1/n8n-nodes-sunx-perps",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepakdhaka-1/n8n-nodes-sunx-perps.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SunxPerpsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SunxPerps/SunxPerps.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
